import pandas as pd

# Convert conditions list to DataFrame
practice_df = pd.DataFrame(practice_conditions)

# Group by sentence ID (e.g., p1, p2, p3, p4) extracted from 'sounds' column
practice_selected_rows = (
    practice_df
    .groupby(practice_df['sounds'].str.extract(r'(p\d)')[0])
    .apply(lambda x: x.sample(1))
    .reset_index(drop=True)
    .to_dict(orient='records')
)
